{
  "name": "@enactprotocol/cli",
  "version": "1.0.10",
  "description": "Official CLI for the Enact Protocol - package, secure, and discover AI tools",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "enact": "./dist/index.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "bun build ../../src/index.ts --outdir ./dist --target node && sed -i.bak '1{/^#!/d;}' ./dist/index.js && echo '#!/usr/bin/env node' | cat - ./dist/index.js > temp && mv temp ./dist/index.js && chmod +x ./dist/index.js",
    "prepublishOnly": "npm run build",
    "test": "cd ../../ && bun test",
    "dev": "cd ../../ && bun run --watch src/index.ts"
  },
  "keywords": [
    "cli",
    "enact",
    "enact-protocol",
    "ai-tools",
    "tool-discovery",
    "command-line-tool"
  ],
  "author": "EnactProtocol",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/EnactProtocol/enact-cli.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/EnactProtocol/enact-cli/issues"
  },
  "homepage": "https://github.com/EnactProtocol/enact-cli#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@clack/core": "^0.4.2",
    "@clack/prompts": "^0.10.1",
    "dotenv": "^16.5.0",
    "picocolors": "^1.1.1",
    "strip-ansi": "^7.1.0",
    "yaml": "^2.8.0",
    "zod": "^3.25.67"
  }
}
